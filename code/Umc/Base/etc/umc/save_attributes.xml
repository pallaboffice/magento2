<save_attributes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Umc_Base:etc/umc/save_attributes.xsd">
    <entity id="umc_module">
        <attribute id="namespace" />
        <attribute id="module_name" />
        <attribute id="version" />
        <attribute id="ui_version" />
        <attribute id="menu_text" />
        <attribute id="menu_parent" />
        <attribute id="sort_order" />
        <attribute id="license" />
        <attribute id="qualified" />
        <attribute id="underscore" />
        <attribute id="annotation" />
        <attribute id="create_composer" />
        <attribute id="composer_name" />
        <attribute id="composer_version" />
        <attribute id="composer_description" />
        <attribute id="composer_license" />
        <attribute id="create_license" />
        <attribute id="create_readme" />
        <attribute id="readme" />
    </entity>
    <entity id="umc_entity">
        <attribute id="label_singular" />
        <attribute id="label_plural" />
        <attribute id="name_singular" />
        <attribute id="name_plural" />
        <attribute id="is_tree" />
        <attribute id="add_created_to_grid" />
        <attribute id="add_updated_to_grid" />
        <attribute id="type" />
        <attribute id="search" />
        <attribute id="inline_edit" />
    </entity>
    <entity id="umc_relation">
        <attribute id="title" />
        <attribute id="code" />
        <attribute id="required" />
    </entity>
</save_attributes>
